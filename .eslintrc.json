{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "google",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "space-before-blocks": "off",
    "require-jsdoc": "off",
    "padded-blocks": "off",
    "indent": "off",
    "max-len": "off",
    "no-trailing-spaces": "off"
  }
}
